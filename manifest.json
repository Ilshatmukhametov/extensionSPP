{
    "manifest_version": 3,
    "name": " не e-com - spp",
    "version": "1.0.1",
    "background": {
        "service_worker": "background.js",
        "type": "module"
      },
    "host_permissions": ["http://*/*", "https://*/*"],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["app.js"],
            "css": ["styles.css"]
        }
    ],
    "permissions": [
        "activeTab",
        "background",
        "debugger",
        "scripting",
        "activeTab",
        "webNavigation",
        "webRequest",
        "tabs"
    ]
}